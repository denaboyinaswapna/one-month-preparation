class Result {
    public static void miniMaxSum(List<Integer> arr) {
    Collections.sort(arr);
    long sum=0;
    int n=arr.size();
    for(int i=0;i<arr.size();i++){
        sum+=arr.get(i);
    }
    long max=sum-arr.get(0);
    long min=sum-arr.get(n-1);
    System.out.println(min+" "+max);
    }
}
